msgid "  q          Quit program without writing partition table"
msgstr "  q          退出程序但不写入分区表"

#: disk-utils/cfdisk.c:2262
msgid "  r          Reduce or enlarge the current partition"
msgstr "  r          减小或扩大当前分区"

