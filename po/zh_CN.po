msgid "  s          Fix partitions order (only when in disarray)"
msgstr "  s          修复分区顺序(仅磁盘阵列)"

#: disk-utils/cfdisk.c:2264
msgid "  t          Change the partition type"
msgstr "  t          更改分区类型"

