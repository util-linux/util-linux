msgid ""
"Units = blocks of 1024 bytes, counting from %d\n"
"\n"
msgstr ""
"Unités= blocs de 1024 octets, décompte à partir de %d\n"
"\n"

#: fdisk/sfdisk.c:953
#, c-format
msgid "   Device Boot   Start       End    #blocks   Id  System\n"
msgstr "   Périph Amorce Début       Fin    #blocs    Id  Système\n"

#: fdisk/sfdisk.c:956
