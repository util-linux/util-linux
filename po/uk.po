#, c-format
msgid "\ttv.tv_sec = %ld, tv.tv_usec = %ld\n"
msgstr "\ttv.tv_sec = %ld, tv.tv_usec = %ld\n"

#: sys-utils/hwclock.c:848 sys-utils/hwclock.c:936
#, c-format
msgid "\ttz.tz_minuteswest = %d\n"
msgstr "\ttz.tz_minuteswest = %d\n"

