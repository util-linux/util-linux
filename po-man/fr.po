#: ../sys-utils/mount.8.adoc:1387
#, no-wrap
msgid "*partition=*"
msgstr "*partition=*"

#. type: Labeled list
#: ../sys-utils/mount.8.adoc:1390
#, no-wrap
msgid "*fileset=*"
msgstr "*fileset=*"

