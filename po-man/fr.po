msgid "chsh - change your login shell"
msgstr "chsh - modifiez votre interpréteur de connexion"

#. type: Plain text
#: ../login-utils/chsh.1.adoc:24
#, fuzzy, no-wrap
#| msgid "*chsh* [*-s* _shell_] [*-l*] [*-h*] [*-v*] [_username_]\n"
