#, no-wrap
msgid "*-d*, *--nodeps*"
msgstr "*-d*, *--nodeps*"

#. type: Plain text
#: ../misc-utils/lsblk.8.adoc:47
msgid ""
"Do not print holder devices or slaves. For example, *lsblk --nodeps /dev/"
"sda* prints information about the sda device only."
msgstr ""

