msgstr ""

#. type: Labeled list
#: ../sys-utils/adjtime_config.5.adoc:31
#, no-wrap
msgid "*drift factor*"
msgstr ""

#. type: Plain text
#: ../sys-utils/adjtime_config.5.adoc:33
