#. type: Plain text
#: ../text-utils/col.1.adoc:73
msgid ""
"Buffer at least _number_ lines in memory. By default, 128 lines are buffered."
msgstr ""

#. type: Labeled list
#: ../text-utils/col.1.adoc:74
#, no-wrap
msgid "*-p*, *--pass*"
msgstr "*-p*, *--pass*"

