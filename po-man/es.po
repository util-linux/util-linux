#, no-wrap
msgid "*-p*, *--office-phone* _office-phone_"
msgstr ""

#. type: Plain text
#: ../login-utils/chfn.1.adoc:45
msgid "Specify your office phone number."
msgstr ""

#. type: Labeled list
#: ../login-utils/chfn.1.adoc:46
#, no-wrap
msgid "*-h*, *--home-phone* _home-phone_"
msgstr ""

#. type: Plain text
