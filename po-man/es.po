msgid "flock(1)"
msgstr "flock(1)"

#. type: Plain text
#: ../sys-utils/flock.1.adoc:37
msgid "flock - manage locks from shell scripts"
msgstr ""

#. type: Plain text
#: ../sys-utils/flock.1.adoc:41
#, fuzzy
