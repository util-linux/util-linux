#| "*findfs*(8),\n"
#| "*lsblk*(8),\n"
#| "*wipefs*(8)\n"
msgid "*libblkid*(3), *findfs*(8), *lsblk*(8), *wipefs*(8)"
msgstr ""
"*libblkid*(3),\n"
"*findfs*(8),\n"
"*lsblk*(8),\n"
"*wipefs*(8)\n"

