#. type: Plain text
#: ../text-utils/hexdump.1.adoc:48
msgid ""
"hexdump - display file contents in hexadecimal, decimal, octal, or ascii"
msgstr ""

#. type: Plain text
#: ../text-utils/hexdump.1.adoc:50
#, fuzzy
#| msgid "*hexdump* _options file_ ...\n"
