msgid "taskset - set or retrieve a process's CPU affinity"
msgstr ""

#. type: Plain text
#: ../schedutils/taskset.1.adoc:40
#, fuzzy
#| msgid "*runuser* [options] *-u* _user_ [[--] _command_ [_argument_...]]\n"
msgid "*taskset* [options] _mask command_ [_argument_...]"
