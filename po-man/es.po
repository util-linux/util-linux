msgid "*agetty* [options] _port_ [_baud_rate_...] [_term_]"
msgstr "*last* [opciones] [_usuario_...] [_tty_...]\n"

#. type: Plain text
#: ../term-utils/agetty.8.adoc:20
#, fuzzy
#| msgid ""
#| "*agetty* opens a tty port, prompts for a login name and invokes the /bin/"
