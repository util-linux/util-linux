#. type: Title =
#: ../sys-utils/umount.8.adoc:27
#, no-wrap
msgid "umount(8)"
msgstr "umount(8)"

#. type: Plain text
#: ../sys-utils/umount.8.adoc:37
#, fuzzy
#| msgid "mount - mount a filesystem"
msgid "umount - unmount filesystems"
msgstr "mount - monta un sistema de ficheros"

