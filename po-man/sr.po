#, no-wrap
msgid ""
"sort -d /etc/passwd -o /tmp/look.dict\n"
"look -t: root:foobar /tmp/look.dict\n"
msgstr ""
"sort -d /etc/passwd -o /tmp/look.dict\n"
"look -t: root:foobar /tmp/look.dict\n"

#. type: Plain text
#: ../misc-utils/look.1.adoc:105
