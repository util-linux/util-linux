#: ../disk-utils/mkfs.minix.8.adoc:91
#, no-wrap
msgid ""
"*fsck*(8),\n"
"*mkfs*(8),\n"
"*reboot*(8)\n"
msgstr ""
"*fsck*(8),\n"
"*mkfs*(8),\n"
"*reboot*(8)\n"

