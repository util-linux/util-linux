#, no-wrap
msgid "*-T*, *--tree* _rel_"
msgstr "*-T*, *--tree* _однос_"

#. type: Plain text
#: ../sys-utils/lsns.8.adoc:72
#, fuzzy
#| msgid ""
#| "Use tree-like output format.  If *process* is given as _rel_, print "
#| "proecss tree(s) in each name space. This is default when *--tree* is not "
