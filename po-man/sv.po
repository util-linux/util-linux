"/dev/fd0d360  (minor = 4)\n"
"/dev/fd0h1200 (minor = 8)\n"
"/dev/fd0D360  (minor = 12)\n"
"/dev/fd0H360  (minor = 12)\n"
"/dev/fd0D720  (minor = 16)\n"
"/dev/fd0H720  (minor = 16)\n"
"/dev/fd0h360  (minor = 20)\n"
"/dev/fd0h720  (minor = 24)\n"
