#: ../misc-utils/lsblk.8.adoc:167
msgid "mailto:mbroz@redhat.com[Milan Broz], mailto:kzak@redhat.com[Karel Zak]"
msgstr ""

#. type: Plain text
#: ../misc-utils/lsblk.8.adoc:173
#, no-wrap
msgid ""
"*ls*(1),\n"
"*blkid*(8),\n"
