#, no-wrap
msgid "mountpoint(1)"
msgstr ""

#. type: Plain text
#: ../sys-utils/mountpoint.1.adoc:12
msgid "mountpoint - see if a directory or file is a mountpoint"
msgstr ""

#. type: Plain text
#: ../sys-utils/mountpoint.1.adoc:16
