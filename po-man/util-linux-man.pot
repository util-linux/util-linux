msgid "uuid - DCE compatible Universally Unique Identifier library"
msgstr ""

#. type: Plain text
#: ../libuuid/man/uuid.3.adoc:48 ../libuuid/man/uuid_clear.3.adoc:48
#: ../libuuid/man/uuid_compare.3.adoc:48 ../libuuid/man/uuid_copy.3.adoc:48
